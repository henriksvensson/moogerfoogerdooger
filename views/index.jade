include mixins/presetstable
include mixins/presetsdropdown

extends layout

block heading
  h1= title
block action
  button.btn(id="testbutton") Close

block content

  div.row
    div.col-xs-10
      h2 Presets
    div.col-xs-2
      a.btn#btnaddpreset(href="addpreset") Add

  p#results

  +presetstable(presets)

  +presetsdropdown(presets)

append scripts
  script(src='/javascripts/index.js')