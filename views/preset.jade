include mixins/controlsdropdown
include mixins/controlstable

extends layout

block action
  a.btn(href="..") Close

block content
  form.form(role="form")
    div.form-group
      label(for="presetname") Preset name:
      input.form-control#presetname(value="jsdf")

  h2 Controls

  +controlstable(controlsinpreset)

  if controlsnotinpreset && controlsnotinpreset.length > 0
    div.row
      div.col-xs-8
        +controlsdropdown(controlsnotinpreset)
      div.col-xs-4 Add

append scripts
  script(src='/javascripts/index.js')