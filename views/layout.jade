doctype html
html(ng-app="myApp")
    head
        title= title
        block styles
        link(rel='stylesheet', href='/bootstrap/css/bootstrap.min.css')
        link(rel='stylesheet', href='/stylesheets/animate/animate.min.css')
        link(rel='stylesheet', href='/stylesheets/style.css')
    body
        div.row
            div.col-xs-12
                block content

        block scripts
            script(src="/javascripts/jquery/jquery.min.js")
            script(src="/bootstrap/js/bootstrap.min.js")
            script(src="/javascripts/angular/angular.min.js")
            script(src='/javascripts/index.js')
            script(src='/javascripts/app.js')
            script(src='/javascripts/services.js')
            script(src='/javascripts/controllers.js')
